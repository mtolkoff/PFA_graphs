				<gammaPrior idref="colPrec.prior"/>
				<normalPrior idref="loadings.prior"/>
				<orderedLatentLiabilityLikelihood idref="liabilityLikelihood"/>
		        <multivariateTraitLikelihood idref="location.traitLikelihood"/>
				<gammaPrior shape="1" scale="0.5" offset="0.0">
					<parameter idref="threshold"/>
				</gammaPrior>
			</prior>
			<likelihood id="likelihood">

	            <latentFactorModel idref="factorModel"/>

			</likelihood>
		</posterior>

		<operators idref="operators"/>

		<log id="screenLog" logEvery="1000">
			<column label="Posterior" dp="4" width="12">
				<posterior idref="posterior"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			 
     
			<column label="threshold" sf="6" width="12">
				<CompoundParameter idref="threshold"/>
			</column>

		</log>

		<log id="fileLog1" logEvery="1000" fileName="AquilegiaFac2.log">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
            <compoundParameter idref="factors1"/>
			<parameter idref="l1"/>
			<parameter idref="l2"/>
			<parameter idref="l3"/>
			<parameter idref="l4"/>
			<parameter idref="l5"/>
			<parameter idref="l6"/>
			<parameter idref="l7"/>
			<parameter idref="l8"/>
			<parameter idref="l9"/>
			<parameter idref="l10"/>
			<parameter idref="l11"/>
			<parameter idref="l12"/>
			<parameter idref="leafTraits"/>
			<parameter idref="colPrec"/>
			<parameter idref="location.precision.diagonal"/>	
			<CompoundParameter idref="threshold"/>


		</log>
		

		
		
		
		

		<logTree id="treeFileLog" logEvery="10000" nexusFormat="true" fileName="AquileguiaFac2.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
